{
  "expo": {
    "name": "Basketry",
    "slug": "basketry",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/basketrylogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#528357"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ravenx.basketry"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/basket.png",
        "backgroundColor": "#FFD200"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
"android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "package": "com.ravenx.basketry"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/basketryLogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-build-properties",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b6b55434-5f37-4e5f-977c-967c59d7eec3"
      }
    }
  }
}
